package com.calculatrice;

public class PileInterface
{
	private int valeur;
	
	public PileInterface () {
		this.valeur = 0;
	}

	public PileInterface (int valeur) {
		this.valeur = valeur;
	}

	public int getValue () {
		return valeur;
	}

	public void affiche () {
		System.out.print (" "+valeur);
	}
}
